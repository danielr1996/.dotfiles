[alias]
	 #Update mvn version
	 mvnv= !"mvn versions:set -DgenerateBackupPoms=false -DnewVersion=$(git describe |\
	 		cut -d"-" -f1,2)"

	#Log Commits
	l = log --pretty=format:"%C(yellow)%h\\ %C(green)%ad\\ %C(reset)%s%C(auto)%d" --graph --date=relative
	ld = log --pretty=format:"%C(yellow)%h\\ %C(green)%ad\\ %C(reset)%s%C(auto)%d" --graph --date=short

	#List aliases
	la = "!git config -l | grep alias | cut -c 7-"
	
	a = add -A 
	st = status
	s = status -s
	diff = diff --color-words
	co = checkout
	cob = checkout -b
	br = branch
	ci = commit
	c = commit -m
	ca = commit -am
	rb = rebase
	cp=cherry-pick
	cl=clone

	#Remote Repositories	
	p = push	
	f = fetch
	m = merge
	fm = pull
	ff = pull
